{
  "name": "enhanced-preprints-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "crc-32": "^1.2.2",
    "html-react-parser": "^5.1.10",
    "i18next": "^25.0.0",
    "moment": "^2.30.1",
    "next": "^14.0.4",
    "node-fetch": "2",
    "pluralize": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.0.0",
    "typescript-eslint": "^8.49.0",
    "zod": "^4.0.0"
  },
  "scripts": {
    "build": "next build",
    "start": "next start",
    "start:dev": "NODE_OPTIONS='--inspect' next dev",
    "test": "jest",
    "lint": "eslint --ext .tsx,.ts,.json src/ browser-tests/ wiremock/",
    "lint:fix": "eslint --fix --ext .tsx,.ts,.json src/ browser-tests/ wiremock",
    "lint:biome": "biome lint",
    "lint:biome:fix": "biome lint --write",
    "lint-sass": "stylelint **/*.scss",
    "lint-sass:fix": "stylelint --fix **/*.scss",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build",
    "test:browser": "playwright test",
    "test-storybook": "test-storybook",
    "chromatic": "chromatic --only-changed --untraced=package.json --untraced=yarn.lock --exit-zero-on-changes"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@chromatic-com/storybook": "^3",
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@next/eslint-plugin-next": "^15.5.2",
    "@playwright/test": "1.45.0",
    "@storybook/addon-a11y": "8.6.15",
    "@storybook/addon-actions": "8.6.15",
    "@storybook/addon-essentials": "8.6.15",
    "@storybook/addon-interactions": "8.6.15",
    "@storybook/addon-links": "8.6.15",
    "@storybook/addon-viewport": "8.6.15",
    "@storybook/cli": "8.6.15",
    "@storybook/nextjs": "8.6.15",
    "@storybook/node-logger": "8.6.15",
    "@storybook/react": "8.6.15",
    "@storybook/test": "8.6.15",
    "@storybook/test-runner": "^0.22",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/i18next": "^13.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.0",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-i18next": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@typescript-eslint/typescript-estree": "^8.49.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "chromatic": "^13.0.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "^15",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-storybook": "^0.12.0",
    "fetch-mock": "^9.11.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "jest-wip-reporter": "^2.1.0",
    "node-mocks-http": "^1.14.1",
    "postcss": "^8.5.6",
    "prettier": "^3.0.3",
    "prop-types": "^15.8.1",
    "react-app-polyfill": "^3.0.0",
    "sass": "^1.54.5",
    "storybook": "8.6.15",
    "storybook-dark-mode": "^4.0.0",
    "stylelint": "^16.23.0",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-order": "^7.0.0",
    "stylelint-prettier": "^5.0.0",
    "stylelint-selector-bem-pattern": "^4.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "wait-on": "^9.0.0",
    "webpack": "^5.74.0"
  },
  "packageManager": "yarn@4.12.0"
}
